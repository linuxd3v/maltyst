<mjml>
  <mj-head>

    <!-- Import Ubuntu font from Google Fonts -->
    <mj-font name="Ubuntu" href="https://fonts.googleapis.com/css2?family=Ubuntu:wght@400;700&display=swap" />

	<mj-attributes>
		<mj-text font-family="system-ui, Ubuntu, sans-serif"></mj-text>
	</mj-attributes>

    <!-- Define your styles here -->
    <mj-style> 
		/* Reset margins and paddings for paragraphs and list items */
		p, li {
			margin: 0 0 0 0; /* top, right, bottom, left */
			padding: 0;
			line-height: 1.4em;
		}

		/* Hover state for links */
		a:hover {
			color: #45884e !important;
			cursor: pointer;
		}

		*{
			font-family: system-ui, Ubuntu, sans-serif; 
		}		

		/* Customize paragraph styles */
		p{
			font-size: 18px;
			line-height: 1.5em;
			color: #000000;
		}
		p + p {
			padding-top: 1em;
		}
		.no-margin {
			margin: 0;
		}

		/* Footer links styling */
		.footer-links a {
			color: #6d6d6d !important;
			text-decoration: underline !important;
			margin: 0 8px; 
		}
		.footer-links a:hover {
			text-decoration: underline !important;
			color: #45884e !important;
		}


		/* "Made with opensource" links styling */
		.madewith-links span{
			color: #6d6d6d !important;
		}
		.madewith-links a {
			color: #6d6d6d !important;
			text-decoration: none !important;
		}
		.madewith-links a:hover {
			text-decoration: underline !important;
			color: #45884e !important;
		}
    </mj-style>


	<!-- link styles  -->
	<mj-style inline="inline">

	</mj-style>

    <!-- Define Global Styles -->
    <mj-style inline="inline">

    </mj-style>
  </mj-head>






